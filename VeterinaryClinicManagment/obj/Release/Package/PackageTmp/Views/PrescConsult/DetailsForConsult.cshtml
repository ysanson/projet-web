@model IEnumerable<VeterinaryClinicManagment.ViewModels.PrescViewModel>

<div class="table-responsive">
    <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
        <thead>
            <tr>
                <th>
                    Médicament
                </th>
                <th>
                    Posologie
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var m in Model)
            {
                <tr>
                    <td class="mdl-data-table__cell--non-numeric">
                        @Html.DisplayFor(med => m.med.nomMedicament)
                    </td>
                    <td class="mdl-data-table__cell--non-numeric">
                        @Html.DisplayFor(med => m.presc.posologie)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>